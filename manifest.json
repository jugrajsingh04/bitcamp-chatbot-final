{
    "manifest_version": 3,
    "name": "Chatbot Bubble",
    "version": "1.0",
    "permissions": ["activeTab", "scripting", "notifications"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["inject.js"]
      }
    ],
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "web_accessible_resources": [
        {
          "resources": ["images/icon16.png", "images/icon48.png", "images/icon128.png", "chatbot.html"],
          "matches": ["<all_urls>"]
        }
      ]
      
}
  